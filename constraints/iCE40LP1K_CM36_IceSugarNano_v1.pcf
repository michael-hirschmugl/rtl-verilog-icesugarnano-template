# PCF list for iCE40LP1K-CM36 on the IceSugarNano (cleaned)
# Power/GND/CONFIG/SPI/NC are commented out and only for documentation.

set_io --warn-no-port   -iostandard LVCMOS33 GPIO_01    B1   # DPIO,               Bank=3 IceSugarNano PMOD1 Pin 3 PB1 PMOD3 Pin 4 PB1 (possible differential pair)
set_io --warn-no-port   -iostandard LVCMOS33 GPIO_02    A1   # DPIO,               Bank=3 IceSugarNano PMOD1 Pin 4 PA1 PMOD3 Pin 9 PA1 (possible differential pair)
set_io --warn-no-port   -iostandard LVCMOS33 GPIO_03    C2   # DPIO,               Bank=3 IceSugarNano PMOD3 Pin 10 PC2
set_io --warn-no-port   -iostandard LVCMOS33 GPIO_04    E1   # DPIO,               Bank=3 IceSugarNano PMOD3 Pin 3 PE1
		  						        			            
set_io --warn-no-port   -iostandard LVCMOS33 GPIO_05    E2   # PIO,                Bank=2 IceSugarNano PMOD1 Pin 1 PE2
set_io --warn-no-port   -iostandard LVCMOS33 GPIO_06    E3   # PIO,                Bank=2 IceSugarNano PMOD3 Pin 11 PE3
