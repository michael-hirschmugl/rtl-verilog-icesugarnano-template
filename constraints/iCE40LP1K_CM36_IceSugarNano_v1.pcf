set_io --warn-no-port   -iostandard LVCMOS33 CLK_IN     D1   # IOL_7A_GBIN6  GBIN, Bank=3 IceSugarNano PMOD1 Pin 2 PD1 & ICE_CLK -> MCO (PA8 Pin20 APM32F103)
# set_io --warn-no-port -iostandard LVCMOS33 GPIO_XX    F2   # IOB_35_GBIN5  GBIN, Bank=2 IceSugarNano NOT CONNECTED
set_io --warn-no-port   -iostandard LVCMOS33 GPIO_10    C5   # IOR_60_GBIN3  GBIN, Bank=1 IceSugarNano PMOD2 Pin 4 PC5
set_io --warn-no-port   -iostandard LVCMOS33 GPIO_11    B4   # IOR_61_GBIN2  GBIN, Bank=1 IceSugarNano PMOD3 Pin 1 PB4
set_io --warn-no-port   -iostandard LVCMOS33 UART_TX    A3   # IOT_84_GBIN1  GBIN, Bank=0 IceSugarNano PMOD2 Pin 2 PA3 & UART1_TX (PA9 Pin21 APM32F103)
set_io --warn-no-port   -iostandard LVCMOS33 UART_RX    B3   # IOT_85_GBIN0  GBIN, Bank=0 IceSugarNano PMOD2 Pin 1 PB3 & UART1_RX (PA10 Pin22 APM32F103)

