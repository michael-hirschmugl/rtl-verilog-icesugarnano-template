set_io --warn-no-port   -iostandard LVCMOS33 GPIO_08    B6   # PIO,                Bank=1 IceSugarNano PMOD2 Pin 3 PB6 & Yellow LED
set_io --warn-no-port   -iostandard LVCMOS33 GPIO_09    C6   # PIO,                Bank=1 IceSugarNano PMOD3 Pin 12 PC6
								        		                  
